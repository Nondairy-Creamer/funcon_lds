\end_layout

\begin_layout Standard
\begin_inset Formula $d=\frac{1}{T}\left(\sum_{t=1}^{T}y_{t}-C\sum_{t=1}^{T}m_{t}-D\sum_{t=1}^{T}u_{t}\right)$
\end_inset


\end_layout

\begin_layout Standard
for 
\begin_inset Formula $R$
