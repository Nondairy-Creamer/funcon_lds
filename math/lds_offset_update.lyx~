Set to 0
\end_layout

\begin_layout Standard
For 
\begin_inset Formula $d$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $0=R^{-1}\sum_{t=1}^{T}y_{t}-R^{-1}C\sum_{t=1}^{T}m_{t}-R^{-1}D\sum_{t=1}^{T}u_{t}-R^{-1}Td$
\end_inset


\end_layout

\begin_layout Standard
